def foo (bar: int, baz: float): float {

    var a: float = bar * baz;

    def inner() {
        a = 1;

        def deeper() {
            a = bar * 2;

            def much_deeper() {
                a = baz * 3;
                return a;
            }

            return much_deeper();
        }

        return deeper();
    }

    var b = inner() * 4;
    return b;
}

foo(4, 5);
